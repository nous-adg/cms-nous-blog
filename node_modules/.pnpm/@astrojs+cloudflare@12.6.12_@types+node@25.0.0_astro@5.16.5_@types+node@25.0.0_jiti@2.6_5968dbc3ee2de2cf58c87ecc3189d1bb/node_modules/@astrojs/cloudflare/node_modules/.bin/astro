#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/alessandro_diaz/Documents/Development/Personal/blog-standalone/node_modules/.pnpm/astro@5.16.5_@types+node@25.0.0_jiti@2.6.1_lightningcss@1.30.2_rollup@4.53.3_typescript@5.9.3/node_modules/astro/node_modules:/Users/alessandro_diaz/Documents/Development/Personal/blog-standalone/node_modules/.pnpm/astro@5.16.5_@types+node@25.0.0_jiti@2.6.1_lightningcss@1.30.2_rollup@4.53.3_typescript@5.9.3/node_modules:/Users/alessandro_diaz/Documents/Development/Personal/blog-standalone/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/alessandro_diaz/Documents/Development/Personal/blog-standalone/node_modules/.pnpm/astro@5.16.5_@types+node@25.0.0_jiti@2.6.1_lightningcss@1.30.2_rollup@4.53.3_typescript@5.9.3/node_modules/astro/node_modules:/Users/alessandro_diaz/Documents/Development/Personal/blog-standalone/node_modules/.pnpm/astro@5.16.5_@types+node@25.0.0_jiti@2.6.1_lightningcss@1.30.2_rollup@4.53.3_typescript@5.9.3/node_modules:/Users/alessandro_diaz/Documents/Development/Personal/blog-standalone/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../astro@5.16.5_@types+node@25.0.0_jiti@2.6.1_lightningcss@1.30.2_rollup@4.53.3_typescript@5.9.3/node_modules/astro/astro.js" "$@"
else
  exec node  "$basedir/../../../../../../astro@5.16.5_@types+node@25.0.0_jiti@2.6.1_lightningcss@1.30.2_rollup@4.53.3_typescript@5.9.3/node_modules/astro/astro.js" "$@"
fi
